    "tape": "^4.2.0",
    "zuul": "^3.6.0"
  },
  "dependencies": {
    "create_store": "git://github.com/nathanfaucett/create_store.git",
    "is_primitive": "git://github.com/nathanfaucett/is_primitive.git",
